{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "dockerfile",
    "context": "."
  },
  "deploy": {
    "numReplicas": 1,
    "restartPolicyType": "on-failure",
    "restartPolicyMaxRetries": 5
  },
  "env": {
    "PORT": "5000",
    "PYTHONUNBUFFERED": "1"
  }
}

